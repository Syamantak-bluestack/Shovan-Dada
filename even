
/*Write a program to input 10 integers and check whether all the entered numbers are even
numbers or not.*/
import java.util.*;
class even
{
    public static void main(String args[])
    {
        int flag=0 , n;
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the numbers : ");
        for(int i = 1 ; i<= 10 ; i++)
        {
            n=sc.nextInt();
            if(n%2!=0)
            {
                flag = 1;
            }
        }
        if(flag==0)
        {
            System.out.println("All numbers are  even .");
        }
        else
        {
            System.out.println("All numbers are not even ");
        }
    }
}
